<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.6.xsd">
    <!--
        Added the constraints for entity Servicio.
    -->
    <changeSet id="20220321170231-2" author="jhipster">

        <addForeignKeyConstraint baseColumnNames="usuario_id"
                                 baseTableName="servicio"
                                 constraintName="fk_servicio__usuario_id"
                                 referencedColumnNames="id"
                                 referencedTableName="usuario"/>

        <addForeignKeyConstraint baseColumnNames="servicio_id"
                                 baseTableName="rel_servicio__categoria"
                                 constraintName="fk_rel_servicio__categoria__servicio_id"
                                 referencedColumnNames="id"
                                 referencedTableName="servicio"/>

        <addForeignKeyConstraint baseColumnNames="categoria_id"
                                 baseTableName="rel_servicio__categoria"
                                 constraintName="fk_rel_servicio__categoria__categoria_id"
                                 referencedColumnNames="id"
                                 referencedTableName="categoria"/>
    </changeSet>
</databaseChangeLog>
