<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.6.xsd">
    <!--
        Added the constraints for entity Contrato.
    -->
    <changeSet id="20220321180821-2" author="jhipster">

        <addForeignKeyConstraint baseColumnNames="usuario_id"
                                 baseTableName="contrato"
                                 constraintName="fk_contrato__usuario_id"
                                 referencedColumnNames="id"
                                 referencedTableName="usuario"/>

        <addForeignKeyConstraint baseColumnNames="servicio_id"
                                 baseTableName="contrato"
                                 constraintName="fk_contrato__servicio_id"
                                 referencedColumnNames="id"
                                 referencedTableName="servicio"/>
    </changeSet>
</databaseChangeLog>
